@page "/"






<PageTitle>Index</PageTitle>

<Navbar/>

<div class="container-fluid px-0">
        <div class="row m-0 mb-2">
            <div class="col-12 d-flex flex-md-row flex-column align-items-center justify-content-center">
                <div class="d-flex align-items-center justify-content-center mb-md-0 mb-2">
                    <svg xmlns="http://www.w3.org/2000/svg" focusable="false" role="img" preserveAspectRatio="none"
                        height="28" width="28" viewBox="0 0 80 80" class="jss112" aria-label="OfferUp Logo">
                        <path fill="#00a87e" fill-rule="evenodd"
                            d="M71.68 34.338l-8.79 8.778c-1.523 11.245-11.165 19.915-22.842 19.915C27.313 63.03 16.99 52.719 16.99 40s10.323-23.03 23.058-23.03c11.905 0 21.7 9.01 22.928 20.576l8.704-8.694 8.305 8.296C78.519 16.388 61.205 0 40.048 0 17.93 0 0 17.909 0 40s17.93 40 40.048 40C61.275 80 78.633 63.504 80 42.65l-8.32-8.312z">
                        </path>
                    </svg>
                    <h5 class="default-color font-weight-bold mx-3 my-0">
                        The simpler way to buy and sell locally !
                    </h5>
                </div>
                @*<NavLink class="nav-link" href="singleproductview">
                <span class="oi oi-plus" aria-hidden="true"></span> Product Page
            </NavLink>*@
                <button class="btn default-hover-bg font-weight-bold font-small default-bg text-white rounded-5">
                    Get the app
                </button>
            </div>
        </div>
        <div class="d-flex flex-row align-items-start justify-content-between">
            <div class="px-2 gap-2 custom-scroll w-100">
                <!-- Products go here -->
                <div class="row m-0 px-2">
                    <div class="col my-2 d-flex align-items-center justify-content-center w-100 bg-light">
                        <img src="data:image/png;base64," 
                             class="" style="width: 75%; height: 100%" alt="" />
                    </div>
                </div>
                @*<button @onclick="appendText"> load products</button>*@
            <div class="row m-0 pt-2 products" id="products-container">
            </div>
        </div>
    </div>
</div>
@*<SurveyPrompt Title="How is Blazor working for you?" />*@
@inject IJSRuntime JS
@code{
    //public async void appendText()
    //{
    //    await JS.InvokeAsync<string>("loadProducts");
    //}
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            await JS.InvokeAsync<string>("loadDefaultProducts");
        }
    }
}